# API URL
NEXT_PUBLIC_API_URL='BACKEND_URL'

# App Port
PORT="THIS APP'S PORT"

# Supabase (for client-side auth if needed)
NEXT_PUBLIC_SUPABASE_URL="SUPABASE_URL"
NEXT_PUBLIC_SUPABASE_ANON_KEY="SUPABASE_KEY"

# SSO - Platform
NEXT_PUBLIC_DW_SSO_URL=""

# Required: PostHog Project API Key (starts with phc_)
NEXT_PUBLIC_POSTHOG_KEY=phc_your_project_api_key_here

# Required: PostHog Host URL (e.g., https://us.i.posthog.com or your self-hosted instance)
NEXT_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com

# Optional: For sourcemaps upload (helps with error tracking)
# Personal API Key (starts with phx_)
POSTHOG_PERSONAL_API_KEY=phx_your_personal_api_key_here

# Optional: PostHog Environment ID
POSTHOG_ENV_ID=your_environment_id_here